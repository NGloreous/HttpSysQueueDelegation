{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DelegationConfig": {
    "Rules": [
      {
        "QueueName": "DefaultAppPool",
        "Uri": "http://*:80/",
        "Enabled": true
      },
      {
        "QueueName": "AspnetCoreHelloSelfhost",
        "Uri": "http://*:7000/",
        "Enabled": false
      },
      {
        "QueueName": "AspnetCoreHello",
        "Uri": "http://*:7001/",
        "Enabled": false
      },
      {
        "QueueName": "AspnetFrameworkHello",
        "Uri": "http://*:7002/",
        "Enabled": false
      }
    ]
  }
}
